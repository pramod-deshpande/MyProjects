<MudPaper Outlined="true" Elevation="1" Class="py-3">
    <MudGrid>
        <MudItem md="12">
            <MudContainer Class="d-flex justify-space-between">
                <MudText Typo="Typo.h5"> Project Title</MudText>
                <MudButton Variant="Variant.Outlined" Color="Color.Secondary"><MudText Typo="Typo.body2">More Info</MudText></MudButton>
            </MudContainer>
        </MudItem>
        <MudItem md="12">
            <MudContainer Class="d-flex gap-y-4 flex-column">
                <MudText Typo="Typo.body1"> Project Notes</MudText>
                <MudText Typo="Typo.body1"> Current Work</MudText>
                <MudText Typo="Typo.caption"> GitHib link</MudText>
            </MudContainer>
        </MudItem>
        <MudItem md="12">
            <MudContainer Class="d-flex">
                <MudChip Variant="Variant.Outlined" Color="Color.Primary">Status</MudChip>
                <MudChip Variant="Variant.Outlined" Color="Color.Secondary">Priority</MudChip>
                <MudChip Variant="Variant.Outlined" Color="Color.Dark">Last Draft Date</MudChip>
            </MudContainer>
        </MudItem>
        <MudItem md="12">
            <MudContainer>
                <MudButton Variant="Variant.Filled" DisableElevation="true" Color="Color.Primary"><MudText Typo="Typo.body2">Mark Completed</MudText></MudButton>
            </MudContainer>
        </MudItem>
    </MudGrid>
</MudPaper>





